{
  "name": "supabase-with-openai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "embeddings": "tsx lib/generate-embeddings.ts",
    "embeddings:refresh": "tsx lib/generate-embeddings.ts --refresh",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset --force",
    "seed:from-csv": "tsx scripts/seed/from_csv.ts",
    "seed:validate": "tsx scripts/seed/validate_sources.ts",
    "spec:tokens": "tsx scripts/generate/tokens.ts",
    "test:e2e": "playwright test",
    "percy:ci": "percy exec -- playwright test",
    "percy:local": "percy exec -- playwright test",
    "spec:validate": "node scripts/validate/specs-ajv.mjs",
    "figma:export:build": "cd tools/figma-export-tokens && pnpm i && pnpm build",
    "figma:render:build": "cd tools/figma-plugin && pnpm i && pnpm build",
    "figma:rest:export": "node scripts/figma-rest-export.mjs",
    "db:report": "node scripts/db/report.mjs",
    "build:deterministic": "next build",
    "dev:strict": "NODE_ENV=development next dev -p 3001",
    "api:v1:smoke": "bash tests/e2e/api-v1-smoke.sh http://localhost:3001",
    "test:api:v1:conformance": "playwright test tests/api-conformance.spec.ts",
    "k6:budget": "k6 run --quiet tests/k6-api.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf",
    "sqlKeywordCase": "lower"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-label": "^2.0.1",
    "@supabase/supabase-js": "^2.13.0",
    "@types/react-dom": "18.0.11",
    "ai": "^2.1.3",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "common-tags": "^1.8.2",
    "csv-parse": "6.1.0",
    "dayjs": "1.11.18",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "fast-glob": "3.3.3",
    "github-slugger": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "lucide-react": "^0.128.0",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-mdx": "^2.0.0",
    "mdast-util-to-markdown": "^1.5.0",
    "mdast-util-to-string": "^3.1.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "next": "13.2.4",
    "openai": "^3.3.0",
    "openai-edge": "^1.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animate": "^1.0.5",
    "unist-builder": "^3.0.1",
    "unist-util-filter": "^4.0.1",
    "yargs": "^17.7.2",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@percy/cli": "1.31.2",
    "@percy/playwright": "1.0.9",
    "@playwright/test": "1.55.1",
    "@types/common-tags": "^1.8.1",
    "@types/csv-parse": "1.2.5",
    "@types/estree": "^1.0.0",
    "@types/mdast": "^3.0.11",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/yargs": "^17.0.24",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.31",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=20.11.1 <21",
    "pnpm": ">=9.6.0 <10"
  },
  "packageManager": "pnpm@9.6.0"
}
